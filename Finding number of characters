#include <iostream>
#include <stdio.h>  
#include <string.h>  
using namespace std;
int main()
{
    char str[100];
    int count = 0;
    cin >> str;
    for (int i = 0; i < strlen(str); i++) 
    {
        if (str[i] != ' ')
            count++;
    }
    cout<<"Total number of characters in the word is "<<count;

    return 0;
}
